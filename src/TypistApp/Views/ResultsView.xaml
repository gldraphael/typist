<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="600" d:DesignHeight="450"
             x:Class="TypistApp.Views.ResultsView">

  <DockPanel>
    <TextBlock FontWeight="Bold">Time Elapsed: </TextBlock>
    <TextBlock Text="{Binding TimeElapsedText}" />

    <TextBlock FontWeight="Bold">Net WPM: </TextBlock>
    <TextBlock Text="{Binding NetWPM}" />

    <TextBlock FontWeight="Bold">Time Elapsed: </TextBlock>
    <TextBlock Text="{Binding TimeElapsedText}" />

    <TextBlock FontWeight="Bold">Accuracy: </TextBlock>
    <TextBlock Text="{Binding Accuracy}" />
    
    
  </DockPanel>


</UserControl>
